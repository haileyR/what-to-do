<h2>Register</h2>
<%= form_for :user, url: users_path do |f| %>
<p>
  <%= f.label :username %><br>
  <%= f.text_field :username%>
</p>
<p>
  <%= f.label :password %><br>
  <%= f.text_field :password, type: 'password'  %>
</p>
<p>
  <%= f.label :password_confirmation %><br>
  <%= f.text_field :password_confirmation, type: 'password'  %>
</p>
<p>
  <%= f.submit %>
</p>


<% end %>

